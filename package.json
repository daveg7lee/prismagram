{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "Instagram clone with Express + Prisma +Reach and React Native",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server.js",
    "generate": "prisma generate",
    "prisma": "npm run deploy && npm run generate",
    "deploy": "prisma deploy",
    "prebuild": "yarn run generate",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy api/**/*.graphql ../build/api/",
    "start": "node build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daveg7lee/prismagram.git"
  },
  "author": "dave",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daveg7lee/prismagram/issues"
  },
  "homepage": "https://github.com/daveg7lee/prismagram#readme",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "babel-node": "0.0.1-security",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "nodemon": "^2.0.6"
  }
}
